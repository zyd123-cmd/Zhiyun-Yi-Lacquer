<block wx:if="{{isDataLoaded}}"><view class="article-container"><aifunction vue-id="77627998-1" bind:__l="__l"></aifunction><view class="article-header"><text class="article-title">{{information.title}}</text><text class="article-source">{{information.author}}</text></view><view class="article-content"><block wx:for="{{alternatedContent}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><block wx:if="{{item.type==='text'}}"><text class="article-content-text">{{item.content}}</text></block><block wx:else><view class="article-imageview"><image class="article-image" src="{{item.imageSrc}}" mode="widthFix" data-index="{{index}}" data-event-opts="{{[['tap',[['dealimage',['$event']]]]]}}" bindtap="__e"></image></view></block></view></block></view><view class="example-body" style="display:flex;align-items:center;justify-content:flex-end;margin-right:20rpx;margin-top:20rpx;margin-bottom:20rpx;"><view class="fav-btn-container" style="margin:0 20rpx;"><uni-fav class="favBtn" style="size:100rpx;" vue-id="77627998-2" checked="{{collection}}" bg-color-checked="#dd524d" data-event-opts="{{[['^click',[['oncontent']]]]}}" bind:click="__e" bind:__l="__l"></uni-fav></view><view data-event-opts="{{[['tap',[['onhand',['$event']]]]]}}" class="handup-container" bindtap="__e"><uni-icons vue-id="77627998-3" type="hand-up-filled" size="30rpx" color="{{handupcolor}}" bind:__l="__l"></uni-icons><text style="font-size:24rpx;margin-left:10rpx;color:#666666;text-align:center;">{{'点赞'+displayHandup+''}}</text></view></view></view></block>